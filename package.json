{
  "name": "jucr-poi-importer",
  "version": "0.1.0",
  "private": true,
  "type": "commonjs",
  "engines": {
    "node": ">=20"
  },
  "scripts": {
    "dev": "npm run build && node dist/src/server.js",
    "build": "tsc -p tsconfig.json",
    "typecheck": "tsc -p tsconfig.json --noEmit",
    "lint": "eslint \"src/**/*.ts\" \"tests/**/*.ts\"",
    "check": "npm run lint && npm run typecheck && npm run test:unit",
    "check:e2e": "REQUIRE_MONGO_E2E=1 npm run test:e2e",
    "test": "jest",
    "test:unit": "jest --selectProjects unit",
    "test:e2e": "npm run build && node scripts/run-e2e-with-fake-ocm.js",
    "start:fake-ocm": "npm run build && node dist/src/fake-ocm-server.js",
    "start": "npm run build && node dist/src/server.js"
  },
  "devDependencies": {
    "@eslint/js": "^9.0.0",
    "@types/jest": "^29.5.0",
    "@types/node": "^20.0.0",
    "eslint": "^9.0.0",
    "jest": "^29.7.0",
    "ts-jest": "^29.2.0",
    "typescript": "^5.5.0",
    "typescript-eslint": "^8.0.0"
  },
  "dependencies": {
    "mongodb": "^6.0.0"
  }
}
