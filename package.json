{
  "name": "jucr-poi-importer",
  "version": "0.1.0",
  "private": true,
  "type": "commonjs",
  "engines": {
    "node": ">=20"
  },
  "scripts": {
    "dev": "node dist/server.js",
    "build": "tsc -p tsconfig.json",
    "typecheck": "tsc -p tsconfig.json --noEmit",
    "lint": "eslint .",
    "test": "jest",
    "test:unit": "jest --selectProjects unit",
    "test:e2e": "jest --selectProjects e2e",
    "start:fake-ocm": "npm run build && node dist/fake-ocm-server.js",
    "start": "npm run build && node dist/server.js"
  },
  "devDependencies": {
    "@eslint/js": "^9.0.0",
    "@types/jest": "^29.5.0",
    "@types/node": "^20.0.0",
    "eslint": "^4.1.1",
    "jest": "^19.0.2",
    "ts-jest": "^19.0.14",
    "typescript": "^5.5.0",
    "typescript-eslint": "^8.0.0"
  },
  "dependencies": {
    "mongodb": "^6.0.0"
  }
}
